{  
    "appenders":  
        [  
            {  
                "type":"console",  
                "category":"log_console"  
            },  
            {
               "category": "log_access",
               "type": "DateFile",
               "filename": "./logs/log_access/access.log",
               "pattern": "-yyyy-MM-dd.log",
               "alwaysIncludePattern": true
            },
            {  
                "category":"log_file",  
                "type": "file",  
                "filename": "./logs/log_file/file.log",  
                "maxLogSize": 104800,  
                "backups": 100  
            },  
            {  
                "category":"log_date",  
                "type": "dateFile",  
                "filename": "./logs/log_date/date.log",  
                "alwaysIncludePattern": true,  
                "pattern": "-yyyy-MM-dd-hh.log"  
            }  
        ],  

    "replaceConsole": true,  

    "levels":  
    {  
        "log_access": "INFO", 
        "log_file": "TRACE",  
        "log_date": "ALL",
        "log_console": "ALL"
    }  

}  